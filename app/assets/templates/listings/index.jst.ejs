<div>
  <h2 class='listings-index-title page-header'>Listings</h2>

  <ul class="listings">
    <% listings.each(function (listing) { %>
      <li>
        <a class="listing2" href="#/listings/<%= listing.id %>">
          <h3 class="show"><%= listing.escape("title") %></h3><br>
          <div>
            <img class="image-show" src="<%= listing.get("image") %>"/>
          </div><br><br><br>
        </a>
      </li>
    <% }); %>
  </ul>
</div>
