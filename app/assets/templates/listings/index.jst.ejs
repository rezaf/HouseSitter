<div class='listings-index-title'>
  <h2>All Listings</h2>
</div>

<ul class='listings'>
  <% listings.each(function (listing) { %>
    <li class='listing'>
      <a href="#/listings/<%= listing.id %>">
        <span class='shadow'></span>
          <h4><%= listing.escape("title") %></h4>
        <span class='details'>
          <p><%= listing.escape("description") %></p>
          <p><%= listing.escape("address") %></p><br>
        </span>
        <div>
          <img class="img-responsive" src="<%= listing.get("image") %>"/>
        </div><br>
      </a>
    </li>
  <% }); %>
</ul>
