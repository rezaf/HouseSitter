<div class="listings2">
  <h2 class="show page-header"><%= listing.escape('title') %></h2><br>
  <dl class="dl-horizontal">
    <dt>Description</dt>
    <dd><%= listing.escape('description') %></dd><br>
    <dt>Address</dt>
    <dd><%= listing.escape('address') %></dd><br>
    <dt>Contact email</dt>
    <dd><%= listing.escape('contact_email') %></dd><br>
    <dt >Start date</dt>
    <dd class="datetime-local"><%= moment(listing.escape('start_date')).format('MMMM Do YYYY') %></dd><br>
    <dt>End date</dt>
    <dd class="datetime-local"><%= moment(listing.escape('end_date')).format('MMMM Do YYYY') %></dd><br>
  </dl>
  <div>
    <img class="image-show" src="<%= listing.escape("image") %>"/><br>
  </div>
</div>
